{
    "name": "chris-tenday/unipesa",
    "type": "composer-plugin",
    "description": "Paiement via le gateway Unipesa",
    "keywords": ["composer", "autoload"],
    "homepage": "https://github.com/chris-tenday/unipesa",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Tenday",
            "email": "ctenday@gmail.com"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0 || ^2.0",
        "curl/curl": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "ComposerIncludeFiles\\": "src"
        }
    },
    "extra": {
        "class": "ComposerIncludeFiles\\Plugin"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    }
}
